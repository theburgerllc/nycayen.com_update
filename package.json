    "next": "^14.0.4",
    "react": "^18.2.0",
    "react-calendar": "^6.0.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.58.1",
    "react-select": "^5.10.1",
    "reading-time": "^1.5.0",
