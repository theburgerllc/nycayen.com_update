{
  "name": "nycayen-hair-artistry",
  "version": "1.0.0",
  "description": "Luxury hair & wig design website for Nycayen Moore in NYC",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "perf:audit": "lighthouse http://localhost:3000 --output=html --output-path=./lighthouse-report.html",
    "perf:build-stats": "npx -p webpack-bundle-analyzer webpack-bundle-analyzer .next/static/chunks/*.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:accessibility": "jest --testPathPattern=accessibility",
    "test:performance": "artillery run tests/performance/load-test.js",
    "test:lighthouse": "lhci autorun",
    "test:visual": "playwright test --grep=visual",
    "test:integration": "jest --testPathPattern=integration",
    "test:storybook": "test-storybook",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "lint:staged": "lint-staged",
    "validate-package": "npx npm-package-json-lint .",
    "prepare": "husky install",
    "pre-commit": "npm run lint && npm run type-check && npm run test:ci"
  },
  "dependencies": {
