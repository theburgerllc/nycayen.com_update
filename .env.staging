# Staging Environment Variables
# This file contains environment variable names only - actual values are set in Vercel dashboard

# Application Environment
NODE_ENV=production
NEXT_PUBLIC_ENVIRONMENT=staging
NEXT_PUBLIC_SITE_URL=https://staging-nycayen.vercel.app

# Database Configuration (Staging DB)
DATABASE_URL=
DATABASE_POOL_MIN=1
DATABASE_POOL_MAX=5
DATABASE_TIMEOUT=30000

# Redis Configuration (Staging Redis)
REDIS_URL=
REDIS_PASSWORD=
REDIS_TTL=1800

# Stripe Configuration (Test Keys)
STRIPE_SECRET_KEY=sk_test_
STRIPE_PUBLISHABLE_KEY=pk_test_
STRIPE_WEBHOOK_SECRET=

# Third-party Services (Test/Dev accounts)
FORMSPREE_FORM_ID=test_form_id
TIDIO_PUBLIC_KEY=staging_tidio_key
GOOGLE_ANALYTICS_ID=
GOOGLE_TAG_MANAGER_ID=

# Email Configuration (Test SMTP)
SMTP_HOST=
SMTP_PORT=587
SMTP_USER=
SMTP_PASSWORD=
SMTP_FROM=staging@nycayen.com

# Instagram API (Optional - can use dummy data)
INSTAGRAM_ACCESS_TOKEN=
INSTAGRAM_USER_ID=

# Monitoring & Analytics (Staging project)
SENTRY_DSN=
SENTRY_ORG=
SENTRY_PROJECT=nycayen-staging
VERCEL_ANALYTICS_ID=

# Security (Different keys for staging)
JWT_SECRET=
ENCRYPTION_KEY=
RATE_LIMIT_MAX=200
RATE_LIMIT_WINDOW=900000

# Feature Flags (May differ from production)
FEATURE_ANALYTICS=true
FEATURE_CHAT=false
FEATURE_BOOKING=true
FEATURE_SHOP=true
FEATURE_BLOG=true

# Performance
BUNDLE_ANALYZE=true
ENABLE_SOURCE_MAPS=true
OPTIMIZE_IMAGES=true

# Backup Configuration
BACKUP_SCHEDULE=manual
BACKUP_RETENTION_DAYS=7
BACKUP_STORAGE_PROVIDER=vercel-blob

# Staging-specific
STAGING_DATA_SEED=true
ENABLE_TEST_USERS=true
MOCK_EXTERNAL_APIS=true